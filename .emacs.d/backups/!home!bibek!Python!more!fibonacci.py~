# fibonacci sequence
def fib(x):
    
    "assumes x to be >0 and returns x!"
    if x ==2:
        print 'computing for 2'
    if x==1 or x==0:
        return 1
    elif x<0:
        return None
    else:
        return fib(x-1)+ fib(x-2)

def testFib():
    for x in range(-1,8):
        print fib(x)